@model IEnumerable<Docentes.Models.Publicacion>

@{
    ViewBag.Title = "Index";
}

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Docente.Nombres) Docente
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Editorial.Nombre) Editorial
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tipo.Tipo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Titulo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Año)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Docente.FullName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Editorial.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tipo.Tipo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Titulo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Año)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
